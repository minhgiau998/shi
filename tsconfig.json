{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@features/*": [
        "src/features/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@appTypes/*": [
        "src/types/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@db/*": [
        "src/db/*"
      ]
    }
  }
}